{
  "name": "amateras",
  "version": "0.5.0",
  "description": "Amateras is a DOM Utility library.",
  "module": "index.ts",
  "type": "module",
  "author": {
    "name": "defaultkavy",
    "email": "defaultkavy@gmail.com",
    "url": "https://github.com/defaultkavy"
  },
  "bugs": {
    "url": "https://github.com/defaultkavy/amateras/issues"
  },
  "homepage": "https://github.com/defaultkavy/amateras",
  "license": "MIT",
  "imports": {
    "#core": "./src/core.ts",
    "#node/*": "./src/node/*.ts",
    "#lib/*": "./src/lib/*.ts",
    "#structure/*": "./src/structure/*.ts",
    "#html": "./ext/html/html.ts",
    "#html/*": "./ext/html/node/*.ts"
  },
  "exports": {
    ".": "./src/index.ts",
    "./core": "./src/core.ts",
    "./node/*": "./src/node/*.ts",
    "./lib/*": "./src/lib/*.ts",
    "./structure/*": "./src/structure/*.ts",
    "./html": "./ext/html/html.ts",
    "./html/*": "./ext/html/node/*.ts",
    "./css": "./ext/css/src/index.ts",
    "./css/colors": "./ext/css/src/lib/colors.ts",
    "./css/color/*": "./ext/css/src/lib/colors/*.ts",
    "./router": "./ext/router/index.ts",
    "./ssr": "./ext/ssr/index.ts",
    "./i18n": "./ext/i18n/src/index.ts",
    "./idb": "./ext/idb/src/index.ts",
    "./idb/core": "./ext/idb/src/core.ts",
    "./markdown": "./ext/markdown/index.ts"
  },
  "workspaces": [
    "./ext/*"
  ]
}
