{
  "name": "amateras",
  "version": "0.1.1",
  "description": "Amateras is a DOM Utility library.",
  "module": "index.ts",
  "type": "module",
  "author": {
    "name": "defaultkavy",
    "email": "defaultkavy@gmail.com",
    "url": "https://github.com/defaultkavy"
  },
  "bugs": {
    "url": "https://github.com/defaultkavy/amateras/issues"
  },
  "homepage": "https://github.com/defaultkavy/amateras",
  "license": "MIT",
  "imports": {
    "#core": "./src/core.ts",
    "#node/*": "./src/node/*.ts",
    "#lib/*": "./src/lib/*.ts",
    "#structure/*": "./src/structure/*.ts",
    "#html": "./ext/html/html.ts",
    "#html/*": "./ext/html/node/*.ts"
  },
  "exports": {
    ".": "./src/index.ts",
    "./core": "./src/core.ts",
    "./node": "./src/node/node.ts",
    "./node/*": "./src/node/*.ts",
    "./lib/*": "./src/lib/*.ts",
    "./structure/*": "./src/structure/*.ts",
    "./html": "./ext/html/html.ts",
    "./html/*": "./ext/html/node/*.ts",
    "./css": "./ext/css/src/index.ts",
    "./router": "./ext/router/index.ts",
    "./ssr": "./ext/ssr/index.ts"
  },
  "workspaces": [
    "./ext/*"
  ],
  "peerDependencies": {
    "@amateras/css": "./ext/css"
  }
}
