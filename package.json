{
  "name": "amateras",
  "version": "0.11.0",
  "description": "Amateras is a JavaScript library for building user interface.",
  "module": "index.ts",
  "type": "module",
  "author": {
    "name": "defaultkavy",
    "email": "defaultkavy@gmail.com",
    "url": "https://github.com/defaultkavy"
  },
  "bugs": {
    "url": "https://github.com/defaultkavy/amateras/issues"
  },
  "homepage": "https://github.com/defaultkavy/amateras",
  "license": "MIT",
  "exports": {
    ".": {
      "default": "./index.ts",
      "unpkg": "./build/import-map.js"
    },
    "./env": {
      "browser": "./packages/core/src/env.browser.ts",
      "node": "./packages/core/src/env.node.ts",
      "default": "./packages/core/src/env.browser.ts"
    },
    "./utils": {
      "browser": "./packages/utils/src/index.ts",
      "bun": "./packages/utils/src/index.bun.ts",
      "default": "./packages/utils/src/index.ts"
    },
    "./core": "./packages/core/src/index.ts",
    "./css/*": "./packages/css/src/ext/*.ts",
    "./markdown/syntax/*": "./packages/markdown/src/syntax/*.ts",
    "./*": "./packages/*/src/index.ts"
  },
  "dependencies": {
    "@amateras/core": "./packages/core"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "vite": "^7.3.1",
    "terser": "^5.46.0"
  },
  "files": [
    "packages",
    "index.ts",
    "tsconfig.json",
    "build"
  ]
}
